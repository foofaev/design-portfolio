{
  "name": "design-portfolio",
  "version": "0.0.1",
  "bin": "dist/bin/app.js",
  "main": "frontend/index.tsx",
  "author": "foofaev",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/bin/app.js server",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n table",
    "seedProjects": "npx ts-node --files ./index.ts seedProjects",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx ./",
    "spellcheck": "cspell '**/*.ts[x]'",
    "typecheck": "npx tsc -p . --noEmit",
    "repl": "ts-node bin/app.ts repl",
    "test": "NODE_ENV=test DEBUG=app npx jest --detectOpenHandles",
    "test:backend": "NODE_ENV=test DEBUG=app npx jest /backend/ --detectOpenHandles",
    "front:dev": "npx parcel watch ./frontend/index.html",
    "back:dev": "NODE_ENV=development ts-node-dev ./index.ts server",
    "dev": "yarn run back:dev & yarn run front:dev"
  },
  "jest": {
    "bail": true,
    "testMatch": [ "**/__tests__/**/(*.)+test.[jt]s?(x)"],
    "setupFilesAfterEnv": [
      "jest-extended",
      "<rootDir>__tests__/setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/__tests__/fileMock.ts"
    },
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/__fixtures__",
      "/__tests__/helpers",
      "/dist/"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "**/src/**/*.{ts,tsx}",
      "!**/loader.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "coveragePathIgnorePatterns": [
      ".*\\.d\\.ts"
    ]
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.29",
    "@types/busboy": "^0.2.3",
    "@types/classnames": "^2.2.9",
    "@types/cookie-signature": "^1.0.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.5",
    "@types/lodash": "^4.14.138",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.0.1",
    "@types/parcel-bundler": "^1.12.1",
    "@types/pino": "^6.0.1",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-slick": "^0.23.4",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/react-swipeable-views-utils": "^0.13.2",
    "@types/redux-form": "^8.2.2",
    "@types/sharp": "^0.25.0",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "cspell": "^4.0.47",
    "cspell-dict-russian": "^1.1.17",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.47",
    "assets": "^3.0.1",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.5",
    "classnames": "^2.2.6",
    "commander": "^6.0.0",
    "cookie-signature": "^1.1.0",
    "dotenv": "^8.1.0",
    "fastify": "^2.14.1",
    "fastify-cookie": "^3.6.0",
    "fastify-csrf": "^3.0.0",
    "fastify-helmet": "^3.0.2",
    "fastify-multipart": "^1.0.3",
    "fastify-plugin": "^1.6.0",
    "fastify-sensible": "^2.1.1",
    "fastify-static": "^2.5.0",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.25",
    "moment": "^2.24.0",
    "mysql": "^2.14.1",
    "parcel-bundler": "^1.12.4",
    "pg": "^8.0.0",
