      "redux-form": "^8.2.6",
      "redux-thunk": "^2.3.0",
      "reflect-metadata": "^0.1.13",
      "reselect": "^4.0.0",
      "rollbar": "^2.13.0",
      "sharp": "^0.25.1",
      "typeface-roboto": "^0.0.75",
      "typeface-roboto-slab": "^1.1.3",
      "typeface-roboto-slab-cyrillic": "^0.0.3",
